<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yourdestination">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/mylogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mylogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Addplaces"
            android:theme="@style/AppTheme.NoActionBar"></activity>


        <activity android:name=".Register"
            android:theme="@style/AppTheme.NoActionBar"></activity>


        <activity android:name=".Login"
            android:theme="@style/AppTheme.NoActionBar"></activity>


        <activity
            android:name=".ExploreMoreFloat"
            android:theme="@style/AppTheme.NoActionBar"></activity>

        <activity android:name=".ui.destination.DetailPageActivity" />

        <activity android:name=".SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".GetStarted"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>